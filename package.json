{
  "name": "ug",
  "version": "1.0.0",
  "description": "Web Underground.",
  "main": "index.js",
  "scripts": {
    "test-browser": "karma start --single-run --browsers ChromeHeadlessNoSandbox,FirefoxHeadless karma.config.js",
    "test-node": "mocha --recursive --full-trace tests/node/**/test_*.js",
    "debug-node": "mocha inspect --recursive --full-trace tests/node/**/test_*.js",
    "todo": "node src/cli.js compile todo/app.json",
    "serve-todo": "node src/cli.js serve todo.app",
    "vendor": "for file in webextension-polyfill/dist/browser-polyfill.min.js; do cp node_modules/${file} dist/js/vendor/$(basename ${file}); done",
    "dist": "globify 'src/**/*.js' -u src/cli.js -o dist/js",
    "watch": "globify 'src/**/*.js' -u src/cli.js -w -v -o dist/js"
  },
  "author": "Ben Timby <btimby@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bops": "^1.0.1",
    "bs58": "^4.0.1",
    "bugout": "git+https://github.com/btimby/bugout.git#opts.torrent",
    "cash-dom": "^8.1.0",
    "debug": "^4.3.1",
    "glob-fs": "^0.1.7",
    "is-glob": "^4.0.1",
    "jszip": "^3.5.0",
    "ls-chunk-store": "^2.0.0",
    "tweetnacl": "^1.0.3",
    "webextension-polyfill": "^0.7.0",
    "webtorrent": "^0.112.0",
    "webtorrent-hybrid": "^4.0.3"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "globify": "^3.0.3",
    "karma": "^5.2.3",
    "karma-browserify": "^8.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-opera-launcher": "^1.0.0",
    "mocha": "^8.2.1"
  }
}
