{
  "name": "ug",
  "version": "1.0.0",
  "description": "Web Underground.",
  "main": "index.js",
  "scripts": {
    "test-browser": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless karma.config.js",
    "test-node": "mocha --recursive --full-trace tests/node/**/test_*.js",
    "vendor": "for file in webextension-polyfill/dist/browser-polyfill.min.js; do cp node_modules/${file} dist/js/vendor/$(basename ${file}); done",
    "dist": "globify 'src/*.js' -u src/cli.js -o dist/js",
    "watch": "globify 'src/*.js' -u src/cli.js -w -v -o dist/js"
  },
  "author": "Ben Timby <btimby@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "crypto-js": "^4.0.0",
    "jsrsasign": "^10.1.4",
    "jszip": "^3.5.0",
    "karma-browserify": "^8.0.0",
    "ls-chunk-store": "^2.0.0",
    "webextension-polyfill": "^0.7.0",
    "webtorrent": "^0.112.0"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "globify": "^3.0.3",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^8.2.1"
  }
}
